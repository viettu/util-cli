{
  "name": "util-cli",
  "version": "1.0.0",
  "description": "VietTu's utilities in commandlines",
  "main": "index.js",
  "bin": {
    "util-cli": "./index.js"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "tsc": "tsc --build",
    "build": "ncc build src/index.ts -o dist/util-cli/ && cp package.json dist/util-cli/",
    "cmd:print-text": "node dist/util-cli print-text --text=TEST",
    "cmd:encrypt-config": "node dist/util-cli config --action=encrypt",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "keywords": [
    "VietTu",
    "Utilities"
  ],
  "author": "Viet Tu",
  "license": "ISC",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "axios": "^1.2.3",
    "chalk": "^4.1.2",
    "clipboardy": "^2.3.0",
    "execa": "^5.1.1",
    "js-yaml": "^4.1.0",
    "jsonfile": "^6.1.0",
    "lodash": "^4.17.21",
    "openpgp": "^5.5.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.191",
    "@types/node": "^14.18.11",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "@vercel/ncc": "^0.24.1",
    "commander": "^9.0.0",
    "eslint": "^8.8.0",
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.1.1",
    "typescript": "^4.5.5"
  }
}
